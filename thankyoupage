    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const modalOverlay = document.getElementById('modal-overlay');
            const modalText = document.getElementById('modal-text');
            const modalClose = document.getElementById('modal-close');
            const messageBoxes = document.querySelectorAll('.message-box');

            function openModal(contentId) {
                const contentSource = document.getElementById(contentId);
                if (contentSource) {
                    modalText.innerHTML = contentSource.innerHTML;
                    modalOverlay.style.display = 'flex';
                }
            }

            function closeModal() {
                modalOverlay.style.display = 'none';
            }

            messageBoxes.forEach(box => {
                box.addEventListener('click', function() {
                    const contentTarget = this.getAttribute('data-content-target');
                    openModal(contentTarget);
                });
            });

            modalClose.addEventListener('click', closeModal);
            modalOverlay.addEventListener('click', function(event) {
                if (event.target === modalOverlay) {
                    closeModal();
                }
            });

            document.querySelectorAll('.float-item').forEach(item => {
                const randomDelay = -(Math.random() * 20);
                const randomDuration = 18 + Math.random() * 12; // Duration between 18s and 30s
                item.style.animationDelay = `${randomDelay}s`;
                item.style.animationDuration = `${randomDuration}s`;
            });
        });
    </script>
